<!DOCTYPE html>
<html>
  <head>
    <title><%= title %> - 查看製造課程</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <%- include('video') %>
    <%- include('audio') %>
    <div class="content">
      <h1>學習網站</h1>
      <%- include('header') %>

      <ul>
        <% courses.forEach(course=> { %>
            <li><a href="/courses/myCourses/<%= course._id %>" >Course Name: <%= course.name %>, Author: <%= course.author %>, Money: <%= course.money %> </a>
            </li>
            <% }); %>
      </ul>
    </div>
  </body>
</html>